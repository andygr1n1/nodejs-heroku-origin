{
    "name": "nodejs-heroku-origin",
    "version": "1.0.0",
    "description": "nodejs-heroku-origin",
    "main": "./build/src/index.js",
    "scripts": {
        "build": "rimraf ./build && tsc",
        "start": "node build/src/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "nodejs": "node index.js",
        "tsnode": "ts-node-esm ./src/index.ts ",
        "nodemon": "nodemon ./src/index.ts -e ejs,js,css,html,jpg,png,scss,ts",
        "tsc": "tsc --noEmit",
        "lint": "npx eslint ./src",
        "lint:format": "npx eslint ./src --fix",
        "prettier": "prettier --write ./src",
        "prepare": "husky install",
        "precommit": "yarn run prettier && yarn run lint && git add -A ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run prettier && yarn run lint && git add -A ."
        }
    },
    "keywords": [
        "heroku",
        "andygr1n1",
        "nodejs-heroku-origin"
    ],
    "author": "andygr1n1",
    "license": "ISC",
    "type": "module",
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.14",
        "@types/lodash": "^4.14.190",
        "@types/node": "^18.11.9",
        "@types/nodemon": "^1.19.2",
        "@types/request": "^2.48.8",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "lodash": "^4.17.21",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "@xata.io/client": "^0.21.4",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-jwt": "^7.7.7",
        "husky": "^8.0.2",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^3.0.0",
        "request": "^2.88.2"
    },
    "engines": {
        "node": "18.x"
    }
}
